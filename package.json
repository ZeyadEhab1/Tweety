        "postcss-loader": "^7.3.3",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "tailwindcss": "^3.3.3",
        "vue": "^2.5.17",
        "vue-loader": "^17.2.2",
        "vue-template-compiler": "^2.7.14",
        "webpack-cli": "^5.1.4"
